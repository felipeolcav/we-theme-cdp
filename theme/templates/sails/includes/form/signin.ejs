<form id="form-signin" class="form-horizontal" role="form">
  <div class="form-group">
    <div class="input-group col-xs-24">
      <input type="email" class="form-control input-lg" id="inputUsername" placeholder="Usuário">
    </div>
  </div>
  <div class="form-group">
    <div class="input-group col-xs-24">
      <input type="email" class="form-control input-lg" id="inputEmail" placeholder="E-mail">
    </div>
  </div>
  <div class="form-group">
    <div class="input-group col-xs-12 pull-left">
      <input type="password" class="form-control input-lg" id="inputPassword" placeholder="Senha">
    </div>
    <div class="input-group col-xs-10 pull-right">
      <input type="password" class="form-control input-lg" id="inputPasswordConfirm" placeholder="Confirmar senha">
    </div>
  </div>
  <div class="form-group">
    <div class="col-xs-8 row" style="position: relative;">
      <select class="form-control">
        <option>Mês</option>
        <option>2</option>
        <option>3</option>
        <option>4</option>
        <option>5</option>
      </select>
    </div>
    <div class="col-xs-8 row" style="position: relative; left: 1em;">
      <select class="form-control">
        <option>Dia</option>
        <option>2</option>
        <option>3</option>
        <option>4</option>
        <option>5</option>
      </select>
    </div>
    <div class="col-xs-8 row" style="position: relative; left: 2em;">
      <select class="form-control">
        <option>Ano</option>
        <option>2</option>
        <option>3</option>
        <option>4</option>
        <option>5</option>
      </select>
    </div>
  </div>
  <div class="form-group">
    <div class="radio col-xs-8 row">
      <label>
        <input type="radio" name="optionsRadios" id="optionsRadios1" value="male">
        Masculino
      </label>
    </div>
    <div class="radio col-xs-12">
      <label>
        <input type="radio" name="optionsRadios" id="optionsRadios1" value="female">
        Feminino
      </label>
    </div>
  </div>
  <div class="form-group">
    <div class="col-xs-16 row">
      <button type="submit" class="col-xs-24 btn btn-success">Cadastrar</button>
    </div>
  </div>
</form>
