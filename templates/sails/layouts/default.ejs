<!DOCTYPE html>
<html>
  <head>
    <title><%- title %></title>

    <%- sails.services.theme.getJsScriptTag() %>

    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />

    <meta name="csrf-token" content="<%= _csrf %>" />

    <%- sails.services.theme.getlinkCssTags() %>

    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>

    <style type="text/css">
      @font-face {
        font-family: 'icons-cdp';
        src: url('/bower_components/tupi/icon/cdp/icons-cdp.eot?v=4.2.0');
        src: url('/bower_components/tupi/icon/cdp/icons-cdp.eot?#iefix&v=4.2.0') format('embedded-opentype'), url('/bower_components/tupi/icon/cdp/icons-cdp.woff?v=4.2.0') format('woff'), url('/bower_components/tupi/icon/cdp/icons-cdp.ttf?v=4.2.0') format('truetype'), url('/bower_components/tupi/icon/cdp/icons-cdp.svg?v=4.2.0#fontawesomeregular') format('svg');
        font-weight: normal;
        font-style: normal; }

      @font-face {
        font-family: 'icons-outline';
        src: url('/bower_components/tupi/icon/outline/icons-outline.eot?v=4.2.0');
        src: url('/bower_components/tupi/icon/outline/icons-outline.eot?#iefix&v=4.2.0') format('embedded-opentype'), url('/bower_components/tupi/icon/outline/icons-outline.woff?v=4.2.0') format('woff'), url('/bower_components/tupi/icon/outline/icons-outline.ttf?v=4.2.0') format('truetype'), url('/bower_components/tupi/icon/outline/icons-outline.svg?v=4.2.0#fontawesomeregular') format('svg');
        font-weight: normal;
        font-style: normal; }

      @font-face {
        font-family: 'FontAwesome';
        src: url('/bower_components/tupi/icon/awesome/fontawesome-webfont.eot?v=4.2.0');
        src: url('/bower_components/tupi/icon/awesome/fontawesome-webfont.eot?#iefix&v=4.2.0') format('embedded-opentype'), url('/bower_components/tupi/icon/awesome/fontawesome-webfont.woff?v=4.2.0') format('woff'), url('/bower_components/tupi/icon/awesome/fontawesome-webfont.ttf?v=4.2.0') format('truetype'), url('/bower_components/tupi/icon/awesome/fontawesome-webfont.svg?v=4.2.0#fontawesomeregular') format('svg');
        font-weight: normal;
        font-style: normal; }
    </style>

    <link rel="icon" href="../favicon.ico">
  </head>
  <body>

    <noscript><%- body %></noscript>
  </body>
</html>
