<footer id="footer-main">
  <div class="container">
    <div class="row">

      <!-- blog-section -->
      <div id="footer-main--blog">
        <h4 class="title-footer">
          {{partial 'partials/logo-cdp'}}
        <h4>
        <div class="small-footer">
          <p>Espaço virtual de construção de conhecimento a partir das experiências de cada profissional da Atenção Básica.</p>
        </div>
      </div>
      <!-- /blog-section -->

      <!-- facebook-section -->
      <div id="footer-main--facebook">
        <div class="fb-like-box-lg">
          {{ partial 'partials/facebook-like-box-lg'}}
        </div>
        <div class="fb-like-box-sm">
          {{ partial 'partials/facebook-like-box-sm'}}
        </div>
      </div>
      <!-- /facebook-section -->

      <!-- links-section ::expose links for some sections -->
      <div id="footer-main--links">
        <h4 class="links-section--title title-footer">Links</h4>
        <ul class="links-section--list">
          <!-- help-center-item -->
          <li class="links-section--list-item courses">
            <h5 class="links-section--subtitle">
              <span class="fa-stack">
                <i class="fa fa-circle fa-stack-2x fa-4x pull-left icon-circle-group"></i>
                <i class="cdp cdp-badge cdp-lg cdp-stack-1x cdp-inverse"></i>
              </span>
              <span class="icon-title">Cursos</span>
            </h5>
            <p class="links-section--description">Trocar experiências e fazer uma rede dinâmica e viva.</p>
          </li>

          <li class="links-section--list-item reports">
            <h5 class="links-section--subtitle">
              <span class="fa-stack">
                <i class="fa fa-circle fa-stack-2x fa-4x pull-left icon-circle-group"></i>
                <i class="cdp cdp-report cdp-lg cdp-stack-1x cdp-inverse"></i>
              </span>
              <span class="icon-title">Relatos</span>
            </h5>
            <p class="links-section--description">Trocar experiências e fazer uma rede dinâmica e viva.</p>
          </li>
        </ul>
      </div>
      <!-- /links-section -->

      <!-- sponsor-section -->
      <div id="footer-main--sponsor">
        <h4 class="sponsor-title">{{t 'sponsor.title'}}</h4>
        <ul class="sponsor-list pull-left">
          <li class="sponsor-list--item">
            <a href="#"target="_blank">
              <img class="sponsor-list--item-img" src="http://goo.gl/Vr3AP6">
            </a>
          </li>
          <li class="sponsor-list--item">
            <a href="#"target="_blank">
              <img class="sponsor-list--item-img" src="http://goo.gl/8NGiAV">
            </a>
          </li>
        </ul>
      </div>
      <!-- /sponsor-section -->

      <!-- partner-section -->
      <div id="footer-main--partner">
        <h4 class="partner-title">{{t 'partner.title'}}</h4>
        <ul class="partner-list">
          <li class="partner-list--item">
            <a href="#"target="_blank">
              <img class="partner-list--item-img" src="http://goo.gl/fG7jAl">
            </a>
          </li>
        </ul>
      </div>
      <!-- /partner-section -->

    </div>
  </div>

  <div class="copyright">
    <p class="text-muted">Comunidade de Práticas - <a href="http://creativecommons.org/licenses/by/4.0/" target="_blank">CC</a> 2014</p>
  </div>
</footer>

{{we-avatar-modal store=store}}
